<h3 class="dousiteru">以下の内容で投稿する。</h3>

<%= form_with(model: @tui, url: tuis_path, local: true) do |form| %>
  <%= form.hidden_field :content %>

  <p class="ichiran">つぶやき:<%= @tui.content %></p>
  <p class="ichiran"><%= form.submit  "登録する", class: 'btn btn-primary' %>
  <%= form.submit "戻る", name: 'back', class: 'btn btn-primary' %></p>
<% end %>
